/*

parametri
jnum
sleepSeconds

debug:
%let jnum=01;
%let sleepSeconds=2;

*/
%put ** job num=&jnum;

proc format;
picture dtpic
other='%Y-%0m-%0d__%0H:%0M:%0S' (datatype=datetime)
;
run;

data _null_;
 dt=datetime();
 dtText=put(dt, dtpic.);
 put dtText;
 call symput ( 'dtText', compress( dtText ) );
 put dtText=;
run;


data start;
file _webout;
put "job num=&jnum --- START --- &dtText. ";
put "POD (SYSHOSTNAME==&SYSHOSTNAME";
/* put "SYSPROCESSID=&SYSPROCESSID"; */
run;

data _null_;
call sleep (&sleepSeconds,1);
run;


data _null_;
 dt=datetime();
 dtText=put(dt, dtpic.);
 put dtText;
 call symput ( 'dtText', compress( dtText ) );
 put dtText=;
run;

data end;
file _webout;
put "job num=&jnum --- END   --- &dtText. ";
run;
